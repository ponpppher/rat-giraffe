<p id=notice></p>
<div class="container">
  <ul>
    <br>
    <p><strong>物件名:</strong><%= @feed.propertyname %></p><br />
    <p><strong>賃料:</strong><%= @feed.rent.to_s(:delimited) %>円</p><br />
    <p><strong>住所:</strong><%= @feed.address %></p><br />
    <p><strong>築年数</strong> <%= @feed.age %>年</p><br />
    <p><strong>備考</strong> <%= @feed.remarks %></p><br />
  </ul>
  <p style="border-bottom: double 2px #999999;" align=left ></p>
  <% @feed.nearest_stations.each_with_index do |f, index| %>
    <p><strong>最寄り駅<%= index + 1 %></strong></p>
      <ul>
        <br>
        <p><strong>路線名</strong> <%= f.route_name %><p><br />
        <p><strong>駅名</strong> <%= f.station_name %><p><br />
        <p><strong>徒歩分数</strong> <%= f.minute_walk %>分</p>
        <br />
      </ul>
  <% end %>
  <%= link_to "編集", edit_feed_path(@feed.id) %> | <%= link_to "戻る", root_path %>
</div>
