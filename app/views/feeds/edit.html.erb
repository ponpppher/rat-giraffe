<h1>物件編集</h1>

<%= form_for @feed do |f| %>
  <%= f.label :propertyname %>
  <%= f.text_field :propertyname %>

  <%= f.label :rent %>
  <%= f.number_field :rent %>円

  <%= f.label :address %>
  <%= f.text_field :address %>

  <%= f.label :age %>
  <%= f.number_field :age %>年

  <%= f.label :remarks %>
  <%= f.text_area :remarks %>

  <%= f.submit "更新する" %>
<% end %>


