<p id=notice></p>

<div class="container">
  <h1>物件一覧</h1>
    <table>
      <thead>
        <tr>
          <th class="col-md-1">物件名</th>
          <th class="col-md-1">賃料</th>
          <th class="col-md-2">住所</th>
          <th class="col-md-0">築年数</th>
          <th class="col-md-7">備考</th>
          <th class="col-md-0"><th>
        </tr>
      </thead>
      <tbody>
        <% @feed.each do |f| %>
          <tr>
            <td><%= f.propertyname %></td>
            <td><%= f.rent.to_s(:delimited) %>円</td>
            <td><%= f.address %></td>
            <td><%= f.age %>年</td>
            <td><%= f.remarks %></td>
            <td><%= link_to "詳細", feed_path(f.id) %></td>
            <td><%= link_to "編集", edit_feed_path(f.id) %></td>

            <td><%= link_to "削除", feed_path(id: f.id), method: :delete, data:{confirm:"Are you sure?"} %></td>
          </tr>
        <% end %>
      </tbody>
    <table>
  <br>
  <%= link_to "物件を登録する", new_feed_path %>
</div>
